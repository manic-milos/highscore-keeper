{
  "name": "highscore-keeper",
  "version": "1.0.0",
  "description": "a node application for keeping highscore of different games, to use as is, or as an example for usermade app",
  "main": "src/server/index.js",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --exit -r  ts-node/register './src/**/*.spec.ts'",
    "test-env": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --exit -r  ts-node/register './src/__tests__/**/*'",
    "start": "node ./dist/server/index.js",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/server/index.ts",
    "eslint": "eslint src/**",
    "env-cmd": "env-cmd"
  },
  "author": "manic-milos",
  "license": "ISC",
  "dependencies": {
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.0",
    "@typescript-eslint/parser": "^5.35.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
